{
	"name": "Inputs 0-5M",
	"properties": {
		"activities": [
			{
				"name": "Copy-dbo-Inputs",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "select * from inputs \nwhere input_rid > 0 \nand input_rid <= 5000000"
					},
					"sink": {
						"type": "SqlDWSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"Input_RID": "Input_RID",
							"Input": "Input",
							"Hash": "Hash"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Custom query-i-zg2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dbo-Inputs-o-zg2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}